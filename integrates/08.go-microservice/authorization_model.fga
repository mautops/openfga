model
  schema 1.1

type user

type organization
  relations
    define member: [user]
    define admin: [user]

type document
  relations
    define organization: [organization]
    define owner: [user]
    define editor: [user] or owner
    define viewer: [user] or editor or member from organization
    define can_view: viewer
    define can_edit: editor
    define can_delete: owner
